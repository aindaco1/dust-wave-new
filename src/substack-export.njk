---
pagination:
  data: collections.syndicatableForSubstack
  size: 1
  alias: post
permalink: "substack-export/{{ post.data.title | slugify }}.html"
eleventyExcludeFromCollections: true
---
<p><em>Originally published on {{ post.date | longDate }} at <a href="{{ metadata.url }}/{{ post.data.title | slugify }}.html">dustwave.xyz</a></em></p>

{% if post.data.img %}
<img src="{{ metadata.url }}{{ post.data.img }}" alt="{{ post.data.imgAlt or post.data.title }}">
{% endif %}

{{ post.templateContent | substackClean | safe }}
